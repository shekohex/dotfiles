{
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "json",
    "jsonc",
    "rust",
    "javascript",
    "typescript",
    "markdown",
    "python"
  ],
  "coc.preferences.watchmanPath": "watchman",
  "coc.preferences.formatOnType": false,
  "coc.preferences.floatActions": true,
  "coc.preferences.semanticTokensHighlights": false, // we are using treesetter
  "dialog.pickerButtonShortcut": true,
  "dialog.pickerButtons": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "  ",
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "diagnostic.showUnused": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableSign": true,
  "diagnostic.virtualText": true,
  "codeLens.enable": true,
  "coc.source.file.enable": true,
  "coc.source.issues.filetypes": [
    "gitcommit",
    "gina-commit"
  ],
  "clangd.showDBChangedNotification": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.enableMarkdown": true,
  "git.addGBlameToVirtualText": true,
  "rust-analyzer.assist.importPrefix": "crate",
  "rust-analyzer.highlighting.strings": true,
  "rust-analyzer.assist.importGranularity": "module",
  "rust-analyzer.updates.channel": "nightly",
  "rust-analyzer.hover.linksInHover": true,
  "rust-analyzer.callInfo.full": true,
  "rust-analyzer.assist.importGroup": true,
  "rust-analyzer.diagnostics.enableExperimental": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.extraArgs": ["--tests"],
  "rust-analyzer.cargo.useRustcWrapperForBuildScripts": true,
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.completion.autoimport.enable": true,
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.lens.references": true,
  "rust-analyzer.lens.methodReferences": true,
  "rust-analyzer.hoverActions.enable": true,
  "rust-analyzer.completion.postfix.enable": true,
  "rust-analyzer.inlayHints.enable": true,
  "rust-analyzer.inlayHints.chainingHints": true,
  "rust-analyzer.inlayHints.typeHints": true,
  "rust-analyzer.inlayHints.maxLength": 12,
  "rust-analyzer.debug.runtime": "vimspector",
  "rust-analyzer.debug.vimspector.configuration.name": "launch",
  "rust-analyzer.cargo.autoreload": true,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.procMacro.enable": true,
  "flutter.openDevLogSplitCommand": "set wrap | botright 10split",
  "flutter.autoOpenDevLog": true,
  "flutter.autoHideDevLog": true,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.provider.enableSnippet": true,
  "flutter.lsp.initialization.suggestFromUnimportedLibraries": true,
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": false,
  "prettier.requireConfig": true,
  "prettier.bracketSpacing": true,
  "prettier.formatterPriority": 1,
  "typescript.updateImportsOnFileMove.enable": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "tabnine.disable_file_regex": ["[.]dart$"],
  "bloc.newBlocTemplate": "freezed",
  "bloc.newCubitTemplate": "freezed",
  "bloc.checkForUpdates": true,
  "coc-grammarly.enabled": true
}
